{
  "name": "@textactor/concept-data",
  "version": "0.1.1",
  "description": "Concept data module",
  "main": "./lib/index.js",
  "types": "./types/index.d.ts",
  "scripts": {
    "pretest": "npm run clean && npm run tsc",
    "test": "ava --no-cache --verbose lib/**/*.test.js",
    "posttest": "npm run clean-test-code",
    "tsc": "tsc",
    "tscw": "tsc -w",
    "preversion": "npm run tsc",
    "postversion": "git push --follow-tags",
    "prepublishOnly": "npm run tsc && npm run clean-test-code",
    "clean": "rimraf ./lib && rimraf ./types",
    "clean-test-code": "rimraf ./lib/**/*.test.js && rimraf ./types/**/*.test.d.ts"
  },
  "engines": {
    "node": ">=4.0.0"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/textactor/concept-data.git"
  },
  "keywords": [
    "concept",
    "concept-data"
  ],
  "author": "Dumitru Cantea",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/textactor/concept-data/issues"
  },
  "homepage": "https://github.com/textactor/concept-data#readme",
  "devDependencies": {
    "@types/mongoose": "^5.0.10",
    "@types/ms": "^0.7.30",
    "@types/node": "latest",
    "ava": "^0.23.0",
    "rimraf": "^2.6.2",
    "typescript": "latest"
  },
  "dependencies": {
    "@textactor/concept-domain": "^0.1.0",
    "@textactor/domain": "^0.1.5",
    "mongoose": "^5.0.15"
  }
}
